									"const parseJSON = JSON.parse(decodeuri)\r",
									"try{\r",
									"    const access_token = parseJSON.data.access_token\r",
									"    console.log(access_token)\r",
									"    console.log(\"PM Before\",pm.collectionVariables.get(\"access_token\"))\r",
									"    pm.collectionVariables.set(\"access_token\", access_token);\r",
									"    console.log(\"PM After\",pm.collectionVariables.get(\"access_token\"))\r",
									"}catch(e){\r",
									"    console.error(e.message)\r",
